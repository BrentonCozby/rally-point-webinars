extends ../partials/layout

block head
    include ../partials/head
    +makeHead('Rally Point Webinars | Thinking')

block content
    include ../partials/navbar
    +makeNavbar('thinking')
    header.thinking
        .container
            .row
                .intro-text
                    .intro-heading.col-md-12 webinar thinking.
                    .intro-lead-in.col-md-12
                        p Learn lessons and best practices from the thousands of webinars we have produced.
                    .col-md-12
                        a.page-scroll.btn.btn-xl(href='#media') View Content <span class="carot">&raquo;</span>
    section#media.bg-light-gray
        .container
            .row
                .col-lg-12.text-center
                    h2.section-heading Content
            .thinking-cards
                .thinking-card
                    .text
                        a.title(target='_blank', rel='noopener', href='http://events.rallypointwebinars.com/course/webinar.php?id=2897&source=homepage')
                            h3 An Interview With the &quot;Webinar Guru&quot;
                    .image
                        a(target='_blank', rel='noopener', href='http://events.rallypointwebinars.com/course/webinar.php?id=2897&source=homepage')
                            img(src='images/topsalescover-aaronv2.jpg', alt='Top Sales Cover: Aaron Joslow')
                .thinking-card
                    .text
                        a.title(target='_blank', rel='noopener', href='http://events.rallypointwebinars.com/course/webinar.php?id=271&source=rpweventpage')
                            h3 7 Keys to a Glitch-Free Webinar
                    .image
                        a(target='_blank', rel='noopener', href='http://events.rallypointwebinars.com/course/webinar.php?id=271&source=rpweventpage')
                            img(src='images/7-keys-to-a-glitch-free-webinar.jpg', alt='Running A Glitch-Free Webinar')
                .thinking-card
                    .text
                        a.title(target='_blank', rel='noopener', href='http://www.rallypointwebinars.com/case-studies/generating-fortune-1000-leads-with-events.html ')
                            h3 Generating Fortune 1000 Leads with Hundreds of Global Events
                    .image
                        a(target='_blank', rel='noopener', href='http://www.rallypointwebinars.com/case-studies/generating-fortune-1000-leads-with-events.html ')
                            img(src='images/fortune-1000.jpg', alt='Generating Fortune 1000 leads with 100 global events, without adding staff')
                .thinking-card
                    .text
                        a.title(target='_blank', rel='noopener', href='http://events.rallypointwebinars.com/course/webinar.php?id=651&source=ideaspage')
                            h3 Expand Reach, Grow Revenue: When Lead Generation Webinars Make Business Sense for Your Firm
                    .image
                        a(target='_blank', rel='noopener', href='http://events.rallypointwebinars.com/course/webinar.php?id=651&source=ideaspage')
                            img(src='images/expand-reach-grow.jpg', alt='Expand Reach, Grow Revenue')
                .thinking-card
                    .text
                        a.title(target='_blank', rel='noopener', href='http://events.rallypointwebinars.com/course/webinar.php?id=671&source=ideaspage')
                            h3 How to Keep Good Presentations from Looking Bad
                    .image
                        a(target='_blank', rel='noopener', href='http://events.rallypointwebinars.com/course/webinar.php?id=671&source=ideaspage')
                            img(src='images/good-presentations.jpg', alt='Keeping good presentations from looking bad')
                .thinking-card
                    .text
                        a.title(target='_blank', rel='noopener', href='http://events.rallypointwebinars.com/course/webinar.php?id=1231&source=rpwideaspage')
                            h3 How to Create Powerful Webinar Content Quickly
                    .image
                        a(target='_blank', rel='noopener', href='http://events.rallypointwebinars.com/course/webinar.php?id=1231&source=rpwideaspage')
                            img(src='images/powerful-webinar-content.jpg', alt='Quick guide to quick, powerful webinar content')
    include ../partials/contact
    section.thinking.go-toadditional-content
        .row
            .col-md-12
                p To view addiontal content and resources, click the button below.
        .row
            .col-md-12
                a.btn.btn-xl(href='./additional-content.html') See More Content <span class="carot">&raquo;</span>
    include ../partials/footer
    include ../partials/bottom
